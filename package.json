{
  "name": "n8n-nodes-hubdb",
  "version": "1.0.6",
  "description": "n8n node for HubSpot HubDB API v3 operations",
  "keywords": [
    "n8n-community-node-package",
    "hubspot",
    "hubdb",
    "database"
  ],
  "license": "MIT",
  "homepage": "https://github.com/eduals/n8n-nodes-hubdb#readme",
  "author": {
    "name": "Eduardo Almeida",
    "email": "eduardo.junnior@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eduals/n8n-nodes-hubdb.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint \"nodes/**/*.ts\"",
    "lintfix": "eslint \"nodes/**/*.ts\" --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "nodes": [
      "dist/nodes/HubDb/HubDb.node.js"
    ],
    "credentials": [
      "dist/credentials/HubDbApi.credentials.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.45.0",
    "eslint-plugin-n8n-nodes-base": "~1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "~4.8.4"
  },
  "dependencies": {
    "n8n-workflow": "*"
  }
}

